<LoadPlugin "apache">
  Interval {{.IntervalSeconds}}
</LoadPlugin>
<Plugin "apache">
  <Instance "{{.Name}}[monitorID={{.MonitorID}}]">
    URL "http://{{.Host}}:{{.Port}}/mod_status?auto"
    {{with .Username -}}
    User "{{.}}"
    {{end}}
    {{with .Password}}
    Password "{{.}}"
    {{end}}
  </Instance>
</Plugin>
